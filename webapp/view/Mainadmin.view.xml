<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.globalGym.controller.Mainadmin" xmlns="sap.m" xmlns:f="sap.f" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.commons.layout">
    <Page showHeader="false" title="Page Title" showFooter="false" showNavButton="false" enableScrolling="false">
        <content>
            <layout:AbsoluteLayout horizontalScrolling="Hidden" verticalScrolling="Auto">
                <layout:positions>
                    <layout:PositionContainer left="55px" top="55px">
                        <layout:control>
                            <f:Avatar src="" initials="" displayShape="Circle" displaySize="XL" imageFitType="Cover"/>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="193px" top="63px">
                        <layout:control>
                            <VBox alignItems="Center" direction="Column" fitContainer="false" width="auto" height="auto" justifyContent="Center" renderType="Div" visible="true" displayInline="false">
                                <items>
                                    <Title text="Información" titleStyle="H3" width="auto" textAlign="Begin" visible="true" wrapping="false"/>
                                    <Text text="{Nombre}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true" class="sapUiTinyMargin"/>
                                    <Text text="{Cargo}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true" class="sapUiTinyMargin"/>
                                </items>
                                <layoutData/>
                            </VBox>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="7px" top="210px">
                        <layout:control>
                            <HBox alignItems="Stretch" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="Start" renderType="Div" visible="true" displayInline="false">
                                <items>
                                    <GenericTile header="Ver inventario" subheader="Inventario" frameType="OneByOne" class="sapUiLargeMargin" press="_onGenericTilePress">
                                        <tileContent>
                                            <TileContent footer="" unit="">
                                                <content>
                                                    <NewsContent contentText="Inventario ->" subheader="" class="sapUiMediumMarginTop sapUiSmallMarginBegin"/>
                                                </content>
                                            </TileContent>
                                        </tileContent>
                                    </GenericTile>
                                    <GenericTile header="Historial Prestámos" subheader="historial" frameType="OneByOne" class="sapUiLargeMargin" press="_onGenericTilePress1">
                                        <tileContent>
                                            <TileContent footer="" unit="">
                                                <content>
                                                    <NewsContent contentText="Ver historial ->" subheader="" class="sapUiMediumMarginTop sapUiSmallMarginBegin"/>
                                                </content>
                                            </TileContent>
                                        </tileContent>
                                    </GenericTile>
                                </items>
                                <layoutData/>
                            </HBox>
                        </layout:control>
                    </layout:PositionContainer>
                    <layout:PositionContainer left="1191px" top="15px">
                        <layout:control>
                            <core:Icon src="sap-icon://log" size="38px" color="#286EB4" height="48px" width="48px" visible="true" press="_onIconPress"/>
                        </layout:control>
                    </layout:PositionContainer>
                </layout:positions>
            </layout:AbsoluteLayout>
        </content>
        <footer/>
        <headerContent/>
        <subHeader/>
        <customHeader/>
    </Page>
</mvc:View>
